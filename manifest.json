{
    "applicationName": "Jupyter Server Instance",
    "contactEmail": "ecp@ebi.ac.uk",
    "about": "Cloud Portal application for running bioexcel notebook in Jupyter server instance",
    "version": "2.0",
    "cloudProviders": [
        {
            "cloudProvider": "OSTACK",
            "path": "ostack"
        }
    ],
    "inputs": [
        "disk_image_name"
    ],
    "deploymentParameters": [
        "floating_ip_pool",
        "network_name",
        "repo_url",
        "conda_environment",
        "conda_environment_file_path",
        "python_version",
        "package_install",
        "post_install",
        "docker_install"
    ],
    "outputs": [
        "external_ip",
		"ssh_command"
    ]
}
